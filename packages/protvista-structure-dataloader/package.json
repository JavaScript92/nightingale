{
  "name": "protvista-structure-dataloader",
  "version": "1.0.0",
  "description": "A dataloader and parser (ProtVista compliant) for 3D structures",
  "main": "src/Parser",
  "scripts": {
    "test": "mocha --compilers js:babel-core/register",
    "watch": "npm test -- -w",
    "coverage": "cross-env NODE_ENV=test nyc npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ebi-uniprot/protvista-structure-dataloader.git"
  },
  "keywords": [
    "ProtVista",
    "dataloader",
    "parser",
    "protein-structure"
  ],
  "author": "ljgarcia",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/ebi-uniprot/protvista-structure-dataloader/issues"
  },
  "homepage": "https://github.com/ebi-uniprot/protvista-structure-dataloader#readme",
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-plugin-istanbul": "^4.1.3",
    "babel-preset-es2015": "^6.24.1",
    "browserify": "^14.3.0",
    "chai": "^3.5.0",
    "cross-env": "^5.0.0",
    "fetch-mock": "^5.10.1",
    "mocha": "^3.4.1",
    "nyc": "^10.3.2",
    "sinon": "^2.2.0"
  },
  "dependencies": {
    "node-fetch": "^1.6.3",
    "underscore": "^1.8.3"
  },
  "nyc": {
    "sourceMap": false,
    "instrument": false,
    "exclude": [
      "test/**/*Spec.js",
      "build"
    ],
    "include": [
      "src/**/*.js"
    ],
    "require": [
      "babel-core/register"
    ],
    "reporter": [
      "text",
      "text-summary",
      "lcov"
    ]
  }
}
