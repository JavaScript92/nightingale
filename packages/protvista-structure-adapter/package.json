{
  "name": "protvista-structure-adapter",
  "version": "0.0.1",
  "description": "A dataloader and parser (ProtVista compliant) for 3D structures",
  "main": "src/ProtVistaStructureAdapter",
  "module": "src/index.js",
  "jsnext:main": "src/index.js",
  "scripts": {
    "build": "rimraf ./dist && mkdirp ./dist && rollup -c",
    "rollup-watch": "rollup -cw",
    "browser-sync": "browser-sync start --server -f dist/**/*.*",
    "serve": "npm run rollup-watch & npm run browser-sync",
    "npm-linkall": "npm link uniprot-entry-data-loader uniprot-entry-data-adapter data-loader"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ebi-uniprot/protvista-structure-adapter.git"
  },
  "keywords": [
    "ProtVista",
    "dataloader",
    "parser",
    "protein-structure"
  ],
  "author": "ljgarcia",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/ebi-uniprot/protvista-structure-adapter/issues"
  },
  "homepage": "https://github.com/ebi-uniprot/protvista-structure-adapter#readme",
  "devDependencies": {
    "@webcomponents/webcomponentsjs": "^1.0.19",
    "babel-cli": "^6.24.1",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-preset-env": "^1.6.1",
    "browser-sync": "^2.18.13",
    "mkdirp": "^0.5.1",
    "rimraf": "^2.6.1",
    "rollup": "^0.49.3",
    "rollup-plugin-babel": "^2.7.1",
    "rollup-plugin-node-resolve": "^3.0.0"
  },
  "dependencies": {
    "lodash-es": "^4.17.4"
  }
}
