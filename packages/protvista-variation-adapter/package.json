        "babel-preset-env": "^1.6.1",
        "bower": "^1.8.2",
        "browser-sync": "^2.12.5",
        "cssnano": "^3.10.0",
        "eslint": "^4.12.0",
        "rollup": "^0.49.3",
        "rollup-plugin-babel": "^3.0.2",
