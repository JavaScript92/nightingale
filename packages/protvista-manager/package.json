{
  "name": "protvista-manager",
  "version": "1.0.1",
  "description": "Manager component for the ProtVista tool",
  "main": "dist/protvista-manager.js",
  "module": "src/index.js",
  "jsnext:main": "src/index.js",
  "scripts": {
    "build": "mkdirp ./dist && rollup -c",
    "rollup-watch": "rollup -cw",
