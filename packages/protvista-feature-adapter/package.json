{
  "name": "protvista-feature-adapter",
  "version": "1.0.0",
  "description": "A specialization of the uniprot-entry- data-adapter for ProtVista features (regions and sites)",
  "main": "src/ProtVistaFeatureAdapter",
  "scripts": {
    "build": "rimraf ./dist && mkdirp ./dist && rollup -c",
    "rollup-watch": "rollup -cw",
    "browser-sync": "browser-sync start --server -f dist/**/*.*",
    "serve": "npm run rollup-watch & npm run browser-sync"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ebi-uniprot/protvista-feature-adapter.git"
  },
  "keywords": [
    "ProtVista",
    "dataloader",
    "parser",
    "protein-structure"
  ],
  "author": "ljgarcia",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/ebi-uniprot/protvista-feature-adapter/issues"
  },
  "homepage": "https://github.com/ebi-uniprot/protvista-feature-adapter#readme",
  "devDependencies": {
    "@webcomponents/webcomponentsjs": "^1.0.19",
    "babel-cli": "^6.24.1",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-preset-env": "^1.6.1",
    "browser-sync": "^2.18.13",
    "mkdirp": "^0.5.1",
    "rimraf": "^2.6.1",
    "rollup": "^0.49.3",
    "rollup-plugin-babel": "^2.7.1",
    "rollup-plugin-node-resolve": "^3.0.0"
  },
  "dependencies": {
    "lodash-es": "^4.17.4",
    "uniprot-entry-data-adapter": "^0.0.3",
    "uniprot-entry-data-loader": "^0.0.5"
  }
}
