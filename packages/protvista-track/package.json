{
  "name": "protvista-track",
  "version": "1.0.0",
  "description": "Basic track type of the viewer.",
  "main": "index.js",
  "scripts": {
    "build": "mkdirp ./dist && shx cp ./config/config.json ./dist/ && rollup -c",
    "watch": "nodemon -w src -x \"npm run build\"",
    "serve1": "browser-sync start --server -f dist/**/*.*",
    "build-dev": "rollup -c",
    "build-prod": "NODE_ENV=production rollup -c",
    "rollup-watch": "rollup -cw",
    "test": "mocha",
    "browser-sync": "browser-sync start --server -f dist/**/*.*",
    "serve": "npm run rollup-watch & npm run browser-sync",
    "test-watch": "mocha -w"
  },
  "author": "Gustavo Salazar <gsalazar@ebi.ac.uk>",
  "license": "ISC",
  "devDependencies": {
    "@webcomponents/webcomponentsjs": "^1.0.19",
    "babel-cli": "^6.24.1",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-transform-custom-element-classes": "^0.1.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2016": "^6.24.1",
    "browser-sync": "^2.18.12",
    "chai": "^3.5.0",
    "cssnano": "^3.10.0",
    "eslint": "^4.7.2",
    "mocha": "^3.3.0",
    "nodemon": "^1.11.0",
    "protvista-feature-adapter": "file:..\\protvista-feature-adapter",
    "rollup": "^0.49.3",
    "rollup-plugin-babel": "^2.7.1",
    "rollup-plugin-babel-minify": "^3.1.2",
    "rollup-plugin-conditional": "^1.1.1",
    "rollup-plugin-node-resolve": "^3.0.0",
    "shx": "^0.2.2"
  },
  "dependencies": {
    "d3": "^4.9.1",
    "lodash-es": "^4.17.4",
    "webcomponentsjs": "^1.0.2",
    "data-loader": "file:../data-loader"
  }
}
